func fizzbuzz(let n:i64)->void{
    #*
    for(var i:i64 = 0;i<n;i++){
        if(i%15==0){
            println("fizzbuzz");
        }elif(i%3 == 0){
            println("fizz");
        }elif(i%5 == 0){
            println("buzz");
        }else{
            println(i);
        }
    }
    *#

    {
        var i:i64 = 0;
        loop{
            if(not (i < n)){
                break;
            }
            {
                if(i%15==0){
                    println("fizzbuzz");
                }elif(i%3 == 0){
                    println("fizz");
                }elif(i%5 == 0){
                    println("buzz");
                }else{
                    println(i);
                }
            }
            i=i+1;
        }
    }
}
